{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended"
    /*"airbnb"*/
  ],
  "rules": {
    "quotes": [
      1,
      "single"
    ],
    "semi": [
      1,
      "never"
    ],
    "curly": [
      1,
      "multi-line"
    ],
    "array-bracket-spacing": [
      1,
      "always"
    ],
    "object-curly-spacing": [
      1,
      "always"
    ],
    "arrow-spacing": 1,
    "brace-style": 1,
    "comma-dangle": [
      1,
      "never"
    ],
    "comma-spacing": 1,
    "comma-style": 1
  }
}
